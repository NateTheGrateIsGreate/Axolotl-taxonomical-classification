class Animalia:

    def __init__(self, name): # Animalia class

        self.name = name

        self.characteristics = "Multicellular, eukaryotic, heterotrophic organisms that lack cell walls and reproduce sexually"

class Chordata(Animalia): # Chordata class

    def __init__(self, name):

        super().__init__(name)

        self.characteristics += ", have a notochord, a dorsal hollow nerve cord, pharyngeal slits, and a post-anal tail"
        

class Amphibians(Chordata): # Amphibians class

    def __init__(self, name):

        super().__init__(name)

        self.characteristics += ", are cold-blooded vertebrates that live both in water and on land, have moist, permeable skin and undergoing metamorphosis, transitioning from aquatic larvae to terrestrial adults with lungs"

class axolotl(Amphibians): # Axolotl class

    def __init__(self, name):

        super().__init__(name)

        self.characteristics += ", are carnivorous, have a remarkable ability to regenerate lost or damaged tissues and organs, have feathery external gills, related to salamanders"


